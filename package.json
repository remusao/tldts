{
  "name": "tldts",
  "description": "Library to work against complex domain names, subdomains and URIs.",
  "version": "3.1.2",
  "homepage": "https://github.com/remusao/tldts",
  "author": "RÃ©mi Berson",
  "contributors": [
    "Thomas Parisot (https://oncletom.io)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/remusao/tldts.git"
  },
  "bugs": {
    "url": "https://github.com/remusao/tldts/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "browser": "dist/tldts.umd.js",
  "main": "dist/tldts.cjs.js",
  "module": "dist/tldts.esm.js",
  "types": "dist/tldts.d.ts",
  "files": [
    "dist",
    "lib",
    "tldts.ts"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rfv dist build",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'",
    "update": "git submodule foreach git pull origin master && node ./bin/update.js",
    "build": "tsc -p .",
    "prebuild": "npm run clean",
    "bundle": "rollup -c rollup.config.js",
    "prebundle": "npm run build",
    "minify": "google-closure-compiler --js=./dist/tldts.umd.js --js_output_file=./dist/tldts.umd.min.js",
    "preminify": "npm run bundle",
    "prepack": "npm run minify",
    "test": "jest --coverage --no-cache ./test/",
    "benchmark": "./bin/benchmark.js",
    "prebenchmark": "npm pack"
  },
  "dependencies": {
    "punycode": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "benchmark": "^2.1.4",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "google-closure-compiler": "^20181210.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "rollup": "^1.0.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "keywords": [
    "tld",
    "sld",
    "domain",
    "browser",
    "uri",
    "url",
    "domain name",
    "subdomain",
    "public suffix"
  ]
}
